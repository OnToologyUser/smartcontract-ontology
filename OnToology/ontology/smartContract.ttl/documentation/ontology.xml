    <owl:Ontology rdf:about="http://example.smartcontract.es/sc">
        <owl:versionIRI rdf:resource="http://example.smartcontract.es/sc/0.9"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://example.smartcontract.es/sc#contain -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#contain">
        <rdfs:comment>Indicates if an Attribute can contains other type of Attributes (ex. Struct can contain an Array)</rdfs:comment>
        <rdfs:label>contains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#element -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#element"/>
    


    <!-- http://example.smartcontract.es/sc#hasArrayDimension -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasArrayDimension">
        <rdfs:comment>Relation between the Array and the dimension of this Array</rdfs:comment>
        <rdfs:label>has Array Dimension</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasArrayElement -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasArrayElement">
        <rdfs:comment>Relation between Array and ElementInArray</rdfs:comment>
        <rdfs:label>has Array Element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasAttribute -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasAttribute">
        <rdfs:comment>Indicates if the Contract has Attributes</rdfs:comment>
        <rdfs:label>has Attribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasComplexType -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasComplexType"/>
    


    <!-- http://example.smartcontract.es/sc#hasContractReturn -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasContractReturn"/>
    


    <!-- http://example.smartcontract.es/sc#hasData -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasData">
        <rdfs:comment>Used in simple class for specify the value</rdfs:comment>
        <rdfs:label>has Data</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasFunction -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasFunction">
        <rdfs:comment>Indicates if the Contract has Functions</rdfs:comment>
        <rdfs:label>has Function</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasFunctionModifier -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasFunctionModifier">
        <rdfs:comment>Return of the function</rdfs:comment>
        <rdfs:label>has function modifier</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasMapElement -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasMapElement">
        <rdfs:comment>Indicate the relation between MapValues and Map.</rdfs:comment>
        <rdfs:label>has Map Element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasMapKey -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasMapKey">
        <rdfs:comment>In MapValues, indicate the value of a Key given a Map</rdfs:comment>
        <rdfs:label>has Map Key</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasMapKeyType -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasMapKeyType">
        <rdfs:comment>Used in Map for specify the type of the Map Key type</rdfs:comment>
        <rdfs:label>has Map Key Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasMapValue -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasMapValue">
        <rdfs:comment>In MapValues, indicate the value of a Value given a Map</rdfs:comment>
        <rdfs:label>has Map Value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasMapValueType -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasMapValueType"/>
    


    <!-- http://example.smartcontract.es/sc#hasParameter -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasParameter">
        <rdfs:comment>Indicate if the Function has a parameter (an Attribute value)</rdfs:comment>
        <rdfs:label>has Parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasReturnFunction -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasReturnFunction">
        <rdfs:label>has Function Return</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.smartcontract.es/sc#hasVisibility -->


    <owl:ObjectProperty rdf:about="http://example.smartcontract.es/sc#hasVisibility">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://example.smartcontract.es/sc#arrayTypeData -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#arrayTypeData">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasElement -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasElement"/>
    


    <!-- http://example.smartcontract.es/sc#hasFunctionContent -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasFunctionContent">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Indicates the content of a function</rdfs:comment>
        <rdfs:label>has Function Content</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasGet -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasGet">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates if the Attribute has Getter functions</rdfs:comment>
        <rdfs:label>has getter</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasIndex -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasIndex">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Indicate the size of an ArrayElement and ArrayDimension given an Array</rdfs:comment>
        <rdfs:label>has index</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasInheritance -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasInheritance">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>In SolidityContract, indicate if the Contract has Inheritance of other contract</rdfs:comment>
        <rdfs:label>has inheritance</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasLenght -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasLenght">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Lenght of an Array in ArrayDimension</rdfs:comment>
        <rdfs:label>has lenght</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasName -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of a Contract, Attribute or Function</rdfs:comment>
        <rdfs:label>has name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasSet -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasSet">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicates if the Attribute has Setter functions</rdfs:comment>
        <rdfs:label>has setter</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasValue -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasValue">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Indicate the value of a simple variable (for example, int a = 1, 1 is the value)</rdfs:comment>
        <rdfs:label>has value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#hasVisibility -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#hasVisibility">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#imports -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#imports">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>In ContractSpecification, indicate if the Contract imports other contracts</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#isConstant -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#isConstant">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Indicate if a SimpleAttribute is constant (true) or variable (false)</rdfs:comment>
        <rdfs:label>is constant</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.smartcontract.es/sc#returnType -->


    <owl:DatatypeProperty rdf:about="http://example.smartcontract.es/sc#returnType">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Indicate the return type of a function (the function return a integer, boolean, etc)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://example.smartcontract.es/sc#Array -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#Array">
        <rdfs:subClassOf rdf:resource="http://example.smartcontract.es/sc#Attribute"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasArrayDimension"/>
                <owl:someValuesFrom rdf:resource="http://example.smartcontract.es/sc#ArrayDimension"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasArrayElement"/>
                <owl:someValuesFrom rdf:resource="http://example.smartcontract.es/sc#ElementInArray"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasData"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://example.smartcontract.es/sc#ComplexTypes"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasValue"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Define an Array Attribute</rdfs:comment>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#ArrayDimension -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#ArrayDimension">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasIndex"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasLenght"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Contains the dimension of an Array. If the Array contains more than 1 dimension, then it need index [size] and lenght [value]</rdfs:comment>
        <rdfs:label>Array Dimension</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#Attribute -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#Attribute">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasVisibility"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://example.smartcontract.es/sc#Visibility"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasGet"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasName"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasSet"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Attribute types</rdfs:comment>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#ComplexTypes -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#ComplexTypes">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasComplexType"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://example.smartcontract.es/sc#ContractSpecification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Other type of attributers (like functions)</rdfs:comment>
        <rdfs:label>Complex types</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#ConstantType -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#ConstantType">
        <rdfs:subClassOf rdf:resource="http://example.smartcontract.es/sc#SimpleAttribute"/>
        <rdfs:comment>Define if a SimpleAttribute is a constant (true) or a variable (false)</rdfs:comment>
        <rdfs:label>Constant Type</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#ContractSpecification -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#ContractSpecification">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasAttribute"/>
                <owl:someValuesFrom rdf:resource="http://example.smartcontract.es/sc#Attribute"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasFunction"/>
                <owl:someValuesFrom rdf:resource="http://example.smartcontract.es/sc#Function"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#imports"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasName"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Define the name and the imports of the contract</rdfs:comment>
        <rdfs:label>Contract Specification</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#ElementInArray -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#ElementInArray">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#element"/>
                <owl:someValuesFrom rdf:resource="http://example.smartcontract.es/sc#Attribute"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasIndex"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Contains the values of an Array</rdfs:comment>
        <rdfs:label>Element In Array</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#ElementaryType -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#ElementaryType">
        <rdfs:subClassOf rdf:resource="http://example.smartcontract.es/sc#PrimitiveType"/>
        <rdfs:comment>Contains the elementary types values. Subclass of PrimitiveType</rdfs:comment>
        <rdfs:label>Elementary Type</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#Function -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#Function">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasContractReturn"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://example.smartcontract.es/sc#ContractSpecification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasFunctionModifier"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://example.smartcontract.es/sc#FunctionModifier"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasReturnFunction"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://example.smartcontract.es/sc#FunctionType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasVisibility"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://example.smartcontract.es/sc#Visibility"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasFunctionContent"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasName"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Define a function inside the contract</rdfs:comment>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#FunctionModifier -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#FunctionModifier">
        <rdfs:comment>Function Modifier</rdfs:comment>
        <rdfs:label>Function Modifier</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#FunctionType -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#FunctionType">
        <rdfs:label>Function Type</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#GolangContract -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#GolangContract">
        <rdfs:subClassOf rdf:resource="http://example.smartcontract.es/sc#ContractSpecification"/>
        <owl:disjointWith rdf:resource="http://example.smartcontract.es/sc#SolidityContract"/>
        <rdfs:comment>Indicate if the contract is written in Go</rdfs:comment>
        <rdfs:label>Golang Contract</rdfs:label>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#Map -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#Map">
        <rdfs:subClassOf rdf:resource="http://example.smartcontract.es/sc#Attribute"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasMapElement"/>
                <owl:someValuesFrom rdf:resource="http://example.smartcontract.es/sc#MapElement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasMapKeyType"/>
                <owl:someValuesFrom rdf:resource="http://example.smartcontract.es/sc#ElementaryType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.smartcontract.es/sc#hasMapValueType"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://example.smartcontract.es/sc#ComplexTypes"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Define a Map Attribute</rdfs:comment>
    </owl:Class>
    


    <!-- http://example.smartcontract.es/sc#MapElement -->


    <owl:Class rdf:about="http://example.smartcontract.es/sc#MapElement">
        <rdfs:subClassOf>
